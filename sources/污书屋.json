{
    "name": "污书屋",
    "url": "www.miaoquan2016.com",
    "version": "105",
    "search": {
        "url": "http://www.miaoquan2016.com/search/@post->searchkey=${key}&type=articlename",
        "charset": "utf-8",
        "list": ".branch_menu > .article",
        "name": ".content > h6",
        "author": ".content > .author",
        "cover": ".pic > a > img",
        "detail": "h6 > a"
    },
    "detail": {
        "name": ".cataloginfo > h3",
        "summary": ".intro > p",
        "cover": ".pic > img",
        "catalog": ".infolink > li > p > a:matches(章节目录)"
    },
    "catalog": {
        "list": "ul.chapters > li",
        "name": "a",
        "chapter": "a",
        "page": ".index-container > a:matches(下一页)"
    },
    "chapter": {
        "content": "#novelcontent > .articlecontent",
        "page": "#next_url:matches(下一页)"
    }
}