{
    "name": "艾米小说",
    "url": "www.amxs.cc",
    "version": "103",
    "search": {
        "url": "http://www.amxs.cc/search/@post->searchkey=${key}",
        "charset": "utf-8",
        "list": ".read_book > .bookbox",
        "name": "h4",
        "author": ".author@replace->作者：->",
        "cover": ".bookimg > a > img",
        "detail": "h4 > i > a"
    },
    "detail": {
        "name": "span.title",
        "author": ".author@replace->作者：->",
        "cover": ".synopsisArea_detail > img",
        "catalog": ""
    },
    "catalog": {
        "list": "div:gt(5) > .directoryArea > p",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#chaptercontent > p",
        "page": "#read > p:nth-of-type(2) > #pt_next@equal->下一页"
    }
}
